<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="37dp"
        android:layout_marginEnd="26dp"
        android:text="Latitud"
        app:layout_constraintEnd_toStartOf="@+id/textViewLatitud"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        android:text="Longitud"
        app:layout_constraintBaseline_toBaselineOf="@+id/textViewLongitud"
        app:layout_constraintEnd_toStartOf="@+id/textViewLongitud"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="158dp"
        android:layout_marginEnd="32dp"
        android:text="Altura"
        app:layout_constraintEnd_toStartOf="@+id/textViewAltura"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textViewLatitud"
        android:layout_width="0dp"
        android:layout_height="21dp"
        android:layout_marginEnd="145dp"
        android:text=""
        app:layout_constraintBaseline_toBaselineOf="@+id/textView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView" />

    <TextView
        android:id="@+id/textViewLongitud"
        android:layout_width="0dp"
        android:layout_height="21dp"
        android:layout_marginTop="61dp"
        android:layout_marginEnd="145dp"
        android:layout_marginBottom="60dp"
        android:text=""
        app:layout_constraintBottom_toBottomOf="@+id/textViewAltura"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView2"
        app:layout_constraintTop_toTopOf="@+id/textViewLatitud" />

    <TextView
        android:id="@+id/textViewAltura"
        android:layout_width="0dp"
        android:layout_height="21dp"
        android:layout_marginEnd="145dp"
        android:text=""
        app:layout_constraintBaseline_toBaselineOf="@+id/textView3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView3" />

</android.support.constraint.ConstraintLayout>